#!/usr/bin/env python3
from get_input_args_hints import get_input_args

def main():
    print("Testing get_input_args function...")
    args = get_input_args()
    
    print("\nParsed Command Line Arguments:")
    print(f"Image Directory: {args.dir}")
    print(f"CNN Model Architecture: {args.arch}")
    print(f"Dog Names File: {args.dogfile}")

if __name__ == "__main__":
    main()
    